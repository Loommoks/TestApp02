<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="su.zencode.testapp02">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/item_gallery_toolbar_title"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LaunchActivity"
            android:theme="@style/LauncherTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ItemsGalleryActivity" />
        <activity android:name=".ItemDetailedActivity"
            android:parentActivityName=".ItemsGalleryActivity"></activity>
        <activity
            android:name=".ItemDetailedPagerActivity"
            android:parentActivityName=".ItemsGalleryActivity">
        </activity>
    </application>

</manifest>